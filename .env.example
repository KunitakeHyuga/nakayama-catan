# Database configuration shared between the container and application
POSTGRES_USER=catanatron
POSTGRES_PASSWORD=victorypoint
POSTGRES_DB=catanatron_db
DATABASE_URL=postgresql://catanatron:victorypoint@db:5432/catanatron_db

# Optional OpenAI / negotiation settings exposed by the backend
OPENAI_API_KEY=
NEGOTIATION_ADVICE_MODEL=gpt-5o-mini
NEGOTIATION_ADVICE_FALLBACK_MODEL=gpt-4o-mini

# Frontend build defaults
CTRON_API_URL=https://catapult.example.com
CTRON_PUBLIC_URL=https://catapult.example.com

# Cloudflared hostname (mirrors cloudflared/config.yml)
TUNNEL_HOSTNAME=catapult.example.com
